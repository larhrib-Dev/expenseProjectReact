{
    "extends": [
        "wesbos",
        "prettier/react",
        "prettier"
    ],
    "rules": {
        "no-unused-vars": [
            2,
            {
                "ignoreRestSiblings": true,
                "argsIgnorePattern": "res|nex|^err"
            }
        ],
        "no-debugger": 0,
        "no-alert": 0,
        "no-await-in-loop": 0,
        "no-return-assign": ["error", "except-parens"],
        "no-restricted-syntax": [
            2,
            "ForInStatement",
            "LabeledStatement",
            "WithStatement"
        ]

    }
}